{
  "expo": {
    "name": "smartliftfront",
    "slug": "smartliftfront",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/smartlift_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/smartlift_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/smartlift_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fcavallo.smartliftfront",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "notification": {
      "icon": "./assets/smartlift_logo.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "SmartLift"
    },
    "web": {
      "favicon": "./assets/smartlift_logo.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "77b630ba-5812-4fd7-8edf-ad6a7a315fc1"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
